{
    "name": "phpstandard/container",
    "description": "PHP Dependency Container. PSR-11 implementation.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpStandard\\Container\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vusal Orujov",
            "email": "or.vusal@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.8",
        "squizlabs/php_codesniffer": "^3.7",
        "phpmd/phpmd": "^2.13",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "scripts": {
        "phpstan": "phpstan analyse",
        "phpcs": "phpcs",
        "phpcbf": "phpcbf -v",
        "phpmd": "phpmd src ansi phpmd.xml --suffixes=php",
        "unit-test": "phpunit",
        "code-coverage": "phpunit --coverage-html coverage",
        "analyse": [
            "@phpstan",
            "@phpcs",
            "@unit-test"
        ],
        "fix": [
            "@phpcbf"
        ]
    }
}